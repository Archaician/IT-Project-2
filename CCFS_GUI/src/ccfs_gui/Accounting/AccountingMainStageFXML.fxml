<BorderPane fx:id="outer_borderpane" prefHeight="720.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ccfs_gui.Accounting.AccountingMainStageController">
   <bottom>
      <AnchorPane fx:id="bottom_outer_pane" prefHeight="40.0" prefWidth="1200.0" style="-fx-background-color: #539e4a;" BorderPane.alignment="CENTER" />
   </bottom>
   <left>
      <AnchorPane fx:id="left_outer_pane" prefHeight="570.0" prefWidth="50.0" style="-fx-background-color: #e9e9e9;" BorderPane.alignment="CENTER" />
   </left>
   <right>
      <AnchorPane fx:id="right_outer_pane" prefHeight="570.0" prefWidth="50.0" style="-fx-background-color: #e9e9e9;" BorderPane.alignment="CENTER" />
   </right>
   <top>
      <GridPane fx:id="navigation" prefHeight="40.0" style="-fx-background-color: #539e4a;" BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button mnemonicParsing="false" onAction="#homeButton" prefHeight="30.0" prefWidth="50.0" style="-fx-background-color: #539e4a; -fx-border-color: white;" text="HOME" textAlignment="CENTER" textFill="WHITE" GridPane.halignment="RIGHT" GridPane.valignment="CENTER">
               <font>
                  <Font size="11.0" />
               </font>
               <GridPane.margin>
                  <Insets left="5.0" right="5.0" />
               </GridPane.margin>
            </Button>
         </children>
      </GridPane>
   </top>
   <center>
      <AnchorPane fx:id="container" prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: gray;" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
