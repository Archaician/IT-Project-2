   <right>
      <AnchorPane fx:id="right_outer_pane" prefHeight="570.0" prefWidth="50.0" style="-fx-background-color: #e9e9e9;" BorderPane.alignment="CENTER" />
   </right>
   <top>
      <GridPane prefHeight="40.0" style="-fx-background-color: #539e4a;" BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="CYPRESS CHRISTIAN FOUNDATION SCHOOL" textAlignment="CENTER" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS" GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS">
