                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="LIST OF ACCOUNTS" textAlignment="CENTER" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS" GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS">
