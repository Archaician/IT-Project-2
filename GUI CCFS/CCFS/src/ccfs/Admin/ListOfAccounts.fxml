                  </Text>
               </children>
            </GridPane>
         </top>
         <center>
            <TableView prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
              <columns>
                <TableColumn prefWidth="75.0" text="Name" />
                <TableColumn prefWidth="75.0" text="Username" />
                  <TableColumn prefWidth="75.0" text="Account Type" />
                  <TableColumn prefWidth="75.0" text="Status" />
              </columns>
            </TableView>
         </center>
      </BorderPane>
   </center>
   <left>
      <AnchorPane fx:id="left_outer_pane" prefHeight="570.0" prefWidth="50.0" style="-fx-background-color: #e9e9e9;" BorderPane.alignment="CENTER" />
